{
  "expo": {
    "name": "GeAcad",
    "slug": "app-dadm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "scheme": "appdadm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#9f55ea",
        "foregroundImage": "./assets/images/android-icon-foreground2.png",
        "backgroundImage": "./assets/images/android-icon-background2.png",
        "monochromeImage": "./assets/images/android-icon-monochrome2.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.appdadm"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#9f55ea",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
